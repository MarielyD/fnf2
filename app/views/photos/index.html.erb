<p id="notice"><%= notice %></p>

<div class="container">

<h1><img src="/assets/spot.png" width="140px"alt=""></h1>
<h1><img src="/assets/title.png" width="450px"alt=""></h1>

<%= render 'form', photo: Photo.new %>

</div>

<div class="container">

<% @photos.reverse_each do |photo| %>
  <div class='col-md-4'>
    <%= link_to image_tag(photo.url, class: "img-responsive"), photo %>
    <!-- <%= link_to 'Show', photo %> -->
    <%= link_to 'Destroy', photo, method: :delete, data: { confirm: 'Are you sure?' } %>
  </div>
<% end %>

</div>
