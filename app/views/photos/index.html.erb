<p id="notice"><%= notice %></p>
<div class="header">
  <p class="ami">AMI</p>
  <p class="amie">Your Traveling Friend</p>
</div>
<div class="container">
  <h1 class="lead">PLAN YOUR NEXT TRAVEL DESTINATION</h1>
  <h1 class="lead">ENTER TITLE AND URL HERE</h1>
  <%= render 'form', photo: Photo.new %>
</div>

<div class="container">
  <% @photos.reverse_each do |photo| %>
  <div class='col-md-4'>
    <%= link_to image_tag(photo.url, class: "img-responsive"), photo %>
    <!-- <%= link_to 'Show', photo %> -->
    <%= link_to 'Destroy', photo, method: :delete, data: { confirm: 'Are you sure?' } %>
  </div>
  <% end %>

</div>
